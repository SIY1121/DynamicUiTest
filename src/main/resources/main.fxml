<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<?import javafx.scene.control.SplitPane?>
<BorderPane fx:id="root" prefHeight="400.0" prefWidth="600.0" stylesheets="main.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.Controller">
    <center>
        <SplitPane orientation="VERTICAL">
            <SplitPane>
                <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" fx:id="left"/>
                <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" fx:id="center">
                    <AnchorPane fx:id="content" prefHeight="200.0" prefWidth="200.0">
                        <children>
                            <Slider orientation="VERTICAL" />
                            <Separator prefWidth="200.0" />
                            <Slider layoutX="46.0" layoutY="70.0" />
                            <Button layoutX="46.0" layoutY="88.0" mnemonicParsing="false" onAction="#onClick" text="Button" />
                        </children>
                    </AnchorPane>
                </AnchorPane>
                <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" fx:id="right"/>
            </SplitPane>
            <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" fx:id="bottom"/>
        </SplitPane>
    </center>

</BorderPane>
